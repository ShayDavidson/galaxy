{
  "extends": ["eslint:recommended","prettier", "plugin:import/errors"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": 0,
    "no-debugger": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
